 {
    "name": "TFS Exctractor",
    "version": "1.0",
    "description": "Extract project information from TFS",
	"permissions": ["storage", "downloads", "http://*/", "https://*/"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon16.png",
		"48": "images/icon16.png",
		"128": "images/icon16.png"
	},	
    "manifest_version": 2
  }